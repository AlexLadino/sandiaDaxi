FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ph_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Jet
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    Pilot
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    Coflow
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    Outlet
    {
//         type            fixedValue;
//         value           $internalField;
// 	type            fixedFluxPressure;
//         value           $internalField;  
	type            totalPressure;
        p0              $internalField;
        gamma           1;
        value           $internalField;    
    }
    FarField
    {
//         type            zeroGradient;
	type            totalPressure;
        p0              $internalField;
        gamma           1;
        value           $internalField;        
    }
}


