FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.77;

boundaryField
{
    Jet
    {
        type            fixedValue;
        value           uniform 0.6293;
    }
    Pilot
    {
        type            fixedValue;
        value           uniform 0.734;
    }
    Coflow
    {
        type            fixedValue;
        value           uniform 0.77;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    FarField
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;       
    }
}


